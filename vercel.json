{
  "functions": {
    "src/app/api/episode/[...animeid]/route.js": {
      "maxDuration": 30
    },
    "src/app/api/source/[...animeid]/route.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, stale-while-revalidate=7200"
        }
      ]
    }
  ]
}
